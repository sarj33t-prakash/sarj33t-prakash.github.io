{
    "type": "Scaffold",
    "body": {
        "type": "SafeArea",
        "child": {
            "type": "Column",
            "children": [
                {
                    "type": "Expanded",
                    "flex": 1,
                    "child": {
                        "type": "CustomScrollView",
                        "slivers": [
                            {
                                "type": "SliverAppBar",
                                "leading": {
                                    "type": "Icon",
                                    "data": "back",
                                    "color": "#FFFFFF"
                                },
                                "backgroundColor": "#2376D6",
                                "snap": false,
                                "expandedHeight": 200.0,
                                "floating": true,
                                "pinned": true,
                                "flexibleSpace": {
                                    "title": {
                                        "type": "Text",
                                        "data": "Covid-19 Information Centre",
                                        "style": {
                                            "fontSize": 18.0,
                                            "fontWeight": "bold",
                                            "color": "#FFFFFF"
                                        }
                                    },
                                    "centerTitle": true,
                                    "background": {
                                        "type": "Image.asset"
                                    }
                                }
                            },
                            {
                                "type": "SliverPadding",
                                "padding": "16,16,16,16",
                                "sliver": {
                                    "type": "SliverList",
                                    "delegate": {
                                        "type": "ListTile",
                                        "contentPadding": "4,4,4,4",
                                        "leading": {
                                            "type": "Icon",
                                            "data": "back",
                                            "size": 30.0
                                        },
                                        "title": {
                                            "type": "Text",
                                            "data": "{{msg}}"
                                        }
                                    }
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "Padding",
                    "padding": "8,8,8,8",
                    "child": {
                        "type": "SizedBox",
                        "child": {
                            "type": "Text",
                            "data": "Hello"
                        }
                    }
                }
            ]
        }
    }
}
