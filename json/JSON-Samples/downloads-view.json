                "isNetwork": true,
                "semanticsLabel": "Voith Logo",
                "isForBottomSheet": true
            },
            "actions": [
                {
                    "type": "NotificationWidgetParser",
                    "show_badge": true,
                    "badge_color": "#FFFFFF",
                    "badge_position": "end",
                    "badge_text_color": "#FFFFFF",
                    "notification_icon_color": "#FFFFFF"
