        "api_path": "api\/downloads?sort=priority",
        "background": {
            "fit": "cover",
            "url": "https:\/\/sarj33t-prakash.github.io\/images\/bgimage.png",
            "color": "#FFDD00",
            "margin": "0,10,0,10",
