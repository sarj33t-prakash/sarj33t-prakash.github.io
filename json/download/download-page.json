{
    "type": "SingleChildScrollView",
    "child": {
        "type": "Column",
        "children": [
            {
                "type": "Padding",
                "padding": "12,12,12,12",
                "child": {
                    "type": "Material",
                    "color": "#FFEEEE",
                    "elevation": 1.0,
                    "child": {
                        "type": "Padding",
                        "padding": "12,12,12,12",
                        "child": {
                            "type": "Column",
                            "children": [
                                {
                                    "type": "Row",
                                    "mainAxisAlignment": "spaceBetween",
                                    "crossAxisAlignment": "center",
                                    "children": [
                                        {
                                            "type": "Text",
                                            "data": "{{eventname}}",
                                            "style": {
                                                "color": "#000000",
                                                "fontWeight": "bold",
                                                "fontSize": 18.0
                                            }
                                        },
                                        {
                                            "type": "IconButton",
                                            "icon": "share",
                                            "size": 26.0,
                                            "color": "#F43E20"
                                        }
                                    ]
                                },
                                {
                                    "type": "SizedBox",
                                    "height": 12.0
                                },
                                {
                                    "type": "ClipRRect",
                                    "borderRadius": {},
                                    "child": {
                                        "type": "AspectRatio",
                                        "aspectRatio": 1.6,
                                        "child": {
                                            "type": "NetworkImage",
                                            "src": "{{image}}",
                                            "fit": "fill"
                                        }
                                    }
                                },
                                {
                                    "type": "SizedBox",
                                    "height": 16.0
                                },
                                {
                                    "type": "Row",
                                    "mainAxisAlignment": "spaceBetween",
                                    "children": [
                                        {
                                            "type": "Expanded",
                                            "child": {
                                                "type": "Text",
                                                "data": "Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book.",
                                                "style": {
                                                    "color": "#000000",
                                                    "fontWeight": "normal",
                                                    "fontSize": 12.0
                                                },
                                                "maxLines": 5
                                            }
                                        },
                                        {
                                            "type": "IconButton",
                                            "icon": "share",
                                            "size": 26.0,
                                            "color": "#F43E20"
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                }
            }
        ]
    }
}
