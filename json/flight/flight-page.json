{
    "type": "ListView.builder",
    "itemExtent": 1.0,
    "dataBinding": "namerecords",
    "itemChild": {
        "type": "ListTile",
        "title": {
            "type": "Text",
            "data": {{name}}
            "style": {
                "fontSize": 22.0
            }
        },
        "subTitle": {
            "type": "Text",
            "data": {{job}}
            "style": {
                "fontSize": 20.0
            }
        }
    }
}
