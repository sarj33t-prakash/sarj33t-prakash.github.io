{
    "type": "ListView.builder",
    "itemExtent": 1.0,
    "dataBinding": "namerecords",
    "itemChild": {
        "type": "Row",
        "mainAxisAlignment": "spaceBetween",
        "children": [
            {
                "type": "Text",
                "data": "{{name}}",
                "style": {
                    "fontSize": 22.0
                }
