{
    "type": "ListView.builder",
    "itemExtent": 1.0,
    "dataBinding": "namerecords",
    "itemChild": {
        "type": "Column",
        "children": [
            {
                "type": "Row",
                "mainAxisAlignment": "spaceBetween",
                "children": [
                    {
                        "type": "Text",
                        "data": "{{name}}",
                        "style": {
                            "fontSize": 22.0
                        }
                    },
                    {
                        "type": "Text",
                        "data": "{{level}}",
                        "style": {
                            "fontSize": 22.0
                        }
                    }
                ]
            },
            {
                "type": "Row",
                "mainAxisAlignment": "spaceBetween",
                "children": [
                    {
                        "type": "Text",
                        "data": "{{job}}",
                        "style": {
                            "fontSize": 22.0
                        }
                    }
                ]
            }
        ]
    }
}
